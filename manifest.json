{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "description": "This extension allows you to see all of the interactions you have had with any other Twitter user.",
   "icons": {
      "128": "logo128.png",
      "16": "logo16.png",
      "48": "logo48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Twitter Interactions",
   "permissions": [ "cookies", "activeTab", "https://twitter.com/*", "storage", "https://api.twitter.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
